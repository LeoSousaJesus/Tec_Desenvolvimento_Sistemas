programa {
    funcao inicio() {
        inteiro numero
        escreva("Digite um número inteiro: ")
        leia(numero)
        
        se (numero >= 0) {
            escreva("O número é positivo.")

        se (numero == 0){
            escreva("O número é neutro!")
        }    
        } senao {
            escreva("O número é negativo.")
        }
    }
}